<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.wt.entity.StuInfo" table="stu_info">
        <id name="s_id" column="s_id" type="integer">
            <generator class="native"></generator>
        </id>
        
        <property name="s_name" column="s_name" type="string"/>
        
        <property name="s_sex" column="s_sex" type="string"/>
        
    </class>
</hibernate-mapping>
