<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
">
    <bean name="zhangge" class="com.wt.entity.Person">
        <property name="name" value="zhangge" />
        <property name="words" value="三天不打小鬼子，手都痒痒！" />
    </bean>
    
    <bean name="rod" class="com.wt.entity.Person">
        <property name="name" value="rod" />
        <property name="words" value="世界上有10种人，认识二进制的和不认识二进制的。" />
    </bean>
</beans>
