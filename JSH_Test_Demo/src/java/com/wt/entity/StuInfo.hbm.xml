<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.wt.entity">
    
    <class name="StuInfo" table="stu_info">
        
        <id name="stuId" column="stu_id" type="integer">
            <generator class="native"/>
        </id>
        
        <property name="stuName" column="stu_name" type="string" />
        
        <property name="stuEmail" column="stu_email" type="string" />
        
    </class>
    
</hibernate-mapping>
