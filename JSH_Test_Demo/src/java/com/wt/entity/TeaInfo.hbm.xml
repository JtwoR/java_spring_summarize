<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.wt.entity"> 
    
    <class name="TeaInfo" table="tea_info">
        
        <id name="teaId" column="tea_id" type="integer">
            <generator class="native"/>
        </id>
        
        <property name="teaName" column="tea_name" type="string" />
        
        <property name="teaPassword" column="tea_password" type="string" />
        
    </class>
    
</hibernate-mapping>
